{
    "name": "downtownwork/oauth2-pingfederate",
    "description": "PingFederate OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien BADINO",
            "email": "julien@downtownwork.com",
            "homepage": "https://www.downtownwork.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "pingfederate"
    ],
    "require": {
        "php": "~7.2 || ~8.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "DownTownWork\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DownTownWork\\OAuth2\\Client\\Test\\": "tests/"
        }
    }
}
